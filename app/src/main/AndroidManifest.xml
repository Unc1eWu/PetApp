<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.project.petApp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/dog"
        android:label="@string/app_name"
        android:roundIcon="@drawable/dog"
        android:supportsRtl="true"
        android:theme="@style/Theme.PlantAppUI.NoActionBar">
        <activity android:name=".SplashActivity" android:windowSoftInputMode="adjustPan">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".menu.home.HomeDetailActivity" />
        <activity
            android:name=".MainActivity"
            >

        </activity>
    </application>

</manifest>